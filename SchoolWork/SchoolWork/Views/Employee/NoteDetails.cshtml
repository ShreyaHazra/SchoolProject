                </div>
                <div class="col-sm-3">
                    <h4 style="margin-left:5px">@Html.DisplayFor(modelItem => Model.Note_Id)</h4>
                </div>
                <div class="com-sm-6">
                    <h4 style="margin-left:5px">@Html.DisplayFor(modelItem => Model.School_Id)</h4>
